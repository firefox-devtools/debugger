    "source-map": "^0.5.6",
    "svg-inline-loader": "^0.6.1",
    "svg-inline-react": "^1.0.2",
    "tcomb": "^3.1.0"
  },
  "devDependencies": {
    "@kadira/storybook": "^1.17.1",
    "amd-loader": "0.0.5",
    "babel": "^6.5.2",
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.1.2",
