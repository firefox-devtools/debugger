#!/usr/bin/env bash

DOWNLOADS_PATH=${DOWNLOADS_PATH:-"$HOME/downloads"}

mkdir -p $DOWNLOADS_PATH

if [[ -e $DOWNLOADS_PATH/local-mc5.tar ]]; then
  time docker load -i $DOWNLOADS_PATH/local-mc5.tar;
else
  time docker pull jasonlaster11/local-mc5
  docker images
  time docker save jasonlaster11/local-mc5 > $DOWNLOADS_PATH/local-mc5.tar
fi
