{
  "title": "Debugger",
  "environment": "development",
  "baseWorkerURL": "http://localhost:8000/assets/build/",
  "favicon": "assets/images/favicon.png",
  "theme": "light",
  "dir": "ltr",
  "logging": {
    "client": false,
    "firefoxProxy": false,
    "actions": false
  },
  "features": {
    "watchExpressions": {
      "label": "Watch Expressions",
      "enabled": true
    },
    "chromeScopes": {
      "label": "Chrome Scopes",
      "enabled": false
    },
    "eventListeners": {
      "label": "Event Listeners",
      "enabled": false
    },
    "codeCoverage": {
      "label": "Code Coverage",
      "enabled": false
    },
    "searchModifiers": {
      "label": "Search Modifiers",
      "enabled": true
    },
    "symbolSearch": {
      "label": "Symbol Search",
      "enabled": false
    },
    "functionSearch": {
      "label": "function Search",
      "enabled": true
    },
    "editorPreview": {
      "label": "Editor Preview",
      "enabled": true
    },
    "previewMemberExpressions": {
      "label": "Editor member expression Preview",
      "enabled": true
    }
  },
  "chrome": {
    "debug": true,
    "host": "localhost",
    "port": 9222
  },
  "node": {
    "debug": true,
    "host": "localhost",
    "port": 9229
  },
  "firefox": {
    "webSocketConnection": false,
    "proxyHost": "localhost:9000",
    "webSocketHost": "localhost:6080",
    "mcPath": "./firefox"
  },
  "development": {
    "serverPort": 8000,
    "examplesPort": 7999
  }
